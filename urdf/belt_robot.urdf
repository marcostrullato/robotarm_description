<?xml version="1.0"?>
<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<material name="blue">
		<color rgba="0 0 0.8 1" />
	</material>
	<link name="base">
		<visual>
			<geometry>
				<mesh filename="package://robotarm_description/meshes/belt_arm_ros_base.stl" />
			</geometry>
			<material name="blue" />
		</visual>
	</link>
	<joint name="base_body" type="revolute">
		<parent link="base" />
		<child link="body" />
		<limit effort="30.0" lower="0" upper="3.2114" velocity="0.5" />
		<axis xyz="0 0 1" />
		<!-- <origin xyz="0 0 0.056" /> -->
	</joint>
	<link name="body">
		<visual>
			<geometry>
				<mesh filename="package://robotarm_description/meshes/belt_arm_ros_body.stl" />
			</geometry>
			<material name="blue" />
			<origin rpy="0 0 -1.6057">
			</origin>
		</visual>
	</link>
</robot>
